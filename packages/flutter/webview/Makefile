# Build for web
build-web:
	flutter build web

# Build apk profile release
build-apk:
	flutter build apk --release

# install apk in terminal (usb connected)
install-apk:
	flutter install

# Resolve deps
deps:
	flutter pub get

# Generate icon for apk/ios
# Icon place in assets/icon/icon.png 1024x1024
generate:
	flutter pub run flutter_launcher_icons:main
